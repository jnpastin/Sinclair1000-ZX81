000F	D7		RST	$10		;Call the PRINT routine defined in ROM
0010	3E 31		ld	A,$31		;Load the character code for "L" into the A register.
0012	D7		RST	$10		;Call the PRINT routine defined in ROM
0013	3E 29		ld	A,$29		;Load the character code for "D" into the A register.
